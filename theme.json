{
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"background": true,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"link": false,
			"palette": [
				{
					"color": "#FFD25E",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#000000",
					"name": "On Primary",
					"slug": "on-primary"
				},
				{
					"color": "#FFEFC5",
					"name": "Primary Container",
					"slug": "primary-container"
				},
				{
					"color": "#000000",
					"name": "On Primary Container",
					"slug": "on-primary-container"
				},
				{
					"color": "#000000",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#FFFFFF",
					"name": "On Secondary",
					"slug": "on-secondary"
				},
				{
					"color": "#F5F0E7",
					"name": "Secondary Container",
					"slug": "secondary-container"
				},
				{
					"color": "#000000",
					"name": "On Secondary Container",
					"slug": "on-secondary-container"
				},
				{
					"color": "#ffffff",
					"name": "Surface",
					"slug": "surface"
				},
				{
					"color": "#000000",
					"name": "On Surface",
					"slug": "on-surface"
				},
				{
					"color": "#F1EFEB",
					"name": "Surface Variant",
					"slug": "surface-variant"
				},
				{
					"color": "#4D4944",
					"name": "On Surface Variant",
					"slug": "on-surface-variant"
				}
			],
			"text": true
		},
		"custom": {
			"box-shadow": " rgba(0, 0, 0, 0.03) 0 1px 0, rgba(0, 0, 0, 0.06) 0 3px 6px, rgba(0, 0, 0, 0.06) 0 12px 21px;",
			"spacing": {
				"p-x": "clamp(1rem, -1.1429rem + 5.7143vw, 4rem)",
				"p-y": "clamp(0.5625rem, 0.0268rem + 1.4286vw, 1.3125rem)"
			},
			"typography": {
				"leading": {
					"loose": "2",
					"normal": "1.5",
					"tight": "1.25"
				}
			}
		},
		"layout": {
			"contentSize": "1308px",
			"wideSize": "1308px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Archivo",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400 900",
							"src": [
								"file:./assets/fonts/archivo/archivo-variable-font.ttf"
							]
						}
					],
					"fontFamily": "Archivo, sans-serif",
					"name": "Sans",
					"slug": "sans"
				},
				{
					"fontFamily": "'SF Mono', ui-monospace, Menlo, Consolas, Monaco, monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"name": "Body Small",
					"size": "clamp(0.6875rem, 0.6534rem + 0.1706vw, 0.75rem)",
					"slug": "body-small"
				},
				{
					"name": "Body Medium",
					"size": "clamp(0.75rem, 0.6817rem + 0.3413vw, 0.875rem)",
					"slug": "body-medium"
				},
				{
					"name": "Body Large",
					"size": "clamp(0.875rem, 0.8067rem + 0.3413vw, 1rem)",
					"slug": "body-large"
				},
				{
					"name": "Title Small",
					"size": "clamp(1rem, 0.8214rem + 0.4762vw, 1.25rem)",
					"slug": "title-small"
				},
				{
					"name": "Title Medium",
					"size": "clamp(1.125rem, 0.9464rem + 0.4762vw, 1.375rem)",
					"slug": "title-medium"
				},
				{
					"name": "Title Large",
					"size": "clamp(1.25rem, 1.0714rem + 0.4762vw, 1.5rem)",
					"slug": "title-large"
				},
				{
					"name": "Headline Small",
					"size": "clamp(1.5rem, 1.1429rem + 0.9524vw, 2rem)",
					"slug": "headline-small"
				},
				{
					"name": "Headline Medium",
					"size": "clamp(1.75rem, 1.3929rem + 0.9524vw, 2.25rem)",
					"slug": "headline-medium"
				},
				{
					"name": "Headline Large",
					"size": "clamp(2rem, 1.6429rem + 0.9524vw, 2.5rem)",
					"slug": "headline-large"
				},
				{
					"name": "Display Small",
					"size": "clamp(2rem, 0.9286rem + 2.8571vw, 3.5rem)",
					"slug": "display-small"
				},
				{
					"name": "Display Medium",
					"size": "clamp(2.5rem, 1.4286rem + 2.8571vw, 4rem)",
					"slug": "display-medium"
				},
				{
					"name": "Display Large",
					"size": "clamp(3rem, 1.9286rem + 2.8571vw, 4.5rem)",
					"slug": "display-large"
				}
			],
			"fontStyle": false,
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"spacing": {
					"padding": {
						"bottom": "6px",
						"left": "16px",
						"right": "16px",
						"top": "6px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--body-medium)",
					"fontWeight": "500",
					"letterSpacing": "0.03em",
					"lineHeight": "24px"
				}
			},
			"core/navigation": {
				"color": {
					"text": "var(--wp--preset--color--on-surface)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--body-medium)",
					"fontWeight": "500",
					"letterSpacing": "0.03em",
					"lineHeight": "20px",
					"textTransform": "uppercase"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--surface)",
			"text": "var(--wp--preset--color--on-surface-variant)"
		},
		"elements": {
			"h1": {
				"color": {
					"text": "var(--wp--preset--color--on-surface)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--display-large)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--leading--tight)"
				}
			},
			"h2": {
				"color": {
					"text": "var(--wp--preset--color--on-surface)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--display-medium)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--leading--tight)"
				}
			},
			"h3": {
				"color": {
					"text": "var(--wp--preset--color--on-surface)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--display-small)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--leading--tight)"
				}
			},
			"h4": {
				"color": {
					"text": "var(--wp--preset--color--on-surface)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--headline-large)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--leading--tight)"
				}
			},
			"h5": {
				"color": {
					"text": "var(--wp--preset--color--on-surface)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--headline-medium)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--leading--tight)"
				}
			},
			"h6": {
				"color": {
					"text": "var(--wp--preset--color--on-surface)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sans)",
					"fontSize": "var(--wp--preset--font-size--headline-small)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--leading--tight)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans)",
			"fontSize": "var(--wp--preset--font-size--body-large)",
			"letterSpacing": "0.01em",
			"lineHeight": "var(--wp--custom--typography--leading--normal)"
		}
	},
	"version": 2
}