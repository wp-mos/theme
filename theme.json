{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "background": true,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "link": false,
      "text": true,
      "duotone": [],
      "gradients": [],
      "palette": [
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#FFD25E"
        },
        {
          "name": "On Primary",
          "slug": "on-primary",
          "color": "#000000"
        },
        {
          "name": "Primary Container",
          "slug": "primary-container",
          "color": "#FFEFC5"
        },
        {
          "name": "On Primary Container",
          "slug": "on-primary-container",
          "color": "#000000"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#000000"
        },
        {
          "name": "On Secondary",
          "slug": "on-secondary",
          "color": "#FFFFFF"
        },
        {
          "name": "Secondary Container",
          "slug": "secondary-container",
          "color": "#F5F0E7"
        },
        {
          "name": "On Secondary Container",
          "slug": "on-secondary-container",
          "color": "#000000"
        },
        {
          "name": "Surface",
          "slug": "surface",
          "color": "#ffffff"
        },
        {
          "name": "On Surface",
          "slug": "on-surface",
          "color": "#000000"
        },
        {
          "name": "Surface Variant",
          "slug": "surface-variant",
          "color": "#F1EFEB"
        },
        {
          "name": "On Surface Variant",
          "slug": "on-surface-variant",
          "color": "#4D4944"
        }
      ]
    },
    "typography": {
      "customFontSize": false,
      "dropCap": false,
      "fluid": true,
      "fontStyle": false,
      "fontWeight": true,
      "letterSpacing": true,
      "lineHeight": true,
      "textDecoration": true,
      "textTransform": true,
      "fontFamilies": [
        {
          "fontFamily": "Archivo, sans-serif",
          "name": "Sans",
          "slug": "sans",
          "fontFace": [
            {
              "fontFamily": "Archivo",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "fontWeight": "400 900",
              "src": ["file:./assets/fonts/archivo/archivo-variable-font.ttf"]
            }
          ]
        },
        {
          "fontFamily": "'SF Mono', ui-monospace, Menlo, Consolas, Monaco, monospace",
          "name": "Monospace",
          "slug": "monospace"
        }
      ],
      "fontSizes": [
        {
          "name": "Body Small",
          "slug": "body-small",
          "size": "clamp(0.6875rem, 0.6534rem + 0.1706vw, 0.75rem)"
        },
        {
          "name": "Body Medium",
          "slug": "body-medium",
          "size": "clamp(0.75rem, 0.6817rem + 0.3413vw, 0.875rem)"
        },
        {
          "name": "Body Large",
          "slug": "body-large",
          "size": "clamp(0.875rem, 0.8067rem + 0.3413vw, 1rem)"
        },
        {
          "name": "Title Small",
          "slug": "title-small",
          "size": "clamp(1rem, 0.8214rem + 0.4762vw, 1.25rem)"
        },
        {
          "name": "Title Medium",
          "slug": "title-medium",
          "size": "clamp(1.125rem, 0.9464rem + 0.4762vw, 1.375rem)"
        },
        {
          "name": "Title Large",
          "slug": "title-large",
          "size": "clamp(1.25rem, 1.0714rem + 0.4762vw, 1.5rem)"
        },
        {
          "name": "Headline Small",
          "slug": "headline-small",
          "size": "clamp(1.5rem, 1.1429rem + 0.9524vw, 2rem)"
        },
        {
          "name": "Headline Medium",
          "slug": "headline-medium",
          "size": "clamp(1.75rem, 1.3929rem + 0.9524vw, 2.25rem)"
        },
        {
          "name": "Headline Large",
          "slug": "headline-large",
          "size": "clamp(2rem, 1.6429rem + 0.9524vw, 2.5rem)"
        },
        {
          "name": "Display Small",
          "slug": "display-small",
          "size": "clamp(2rem, 0.9286rem + 2.8571vw, 3.5rem)"
        },
        {
          "name": "Display Medium",
          "slug": "display-medium",
          "size": "clamp(2.5rem, 1.4286rem + 2.8571vw, 4rem)"
        },
        {
          "name": "Display Large",
          "slug": "display-large",
          "size": "clamp(3rem, 1.9286rem + 2.8571vw, 4.5rem)"
        }
      ]
    },
    "layout": {
      "contentSize": "1308px",
      "wideSize": "1308px"
    },
    "spacing": {
      "units": ["%", "px", "em", "rem", "vh", "vw"],
      "blockGap": true
    },
    "custom": {
      "spacing": {
        "p-x": "clamp(1rem, -1.1429rem + 5.7143vw, 4rem)",
        "p-y": "clamp(0.5625rem, 0.0268rem + 1.4286vw, 1.3125rem)"
      },
      "typography": {
        "leading": {
          "tight": "1.25",
          "normal": "1.5",
          "loose": "2"
        }
      },
      "box-shadow": " rgba(0, 0, 0, 0.03) 0 1px 0, rgba(0, 0, 0, 0.06) 0 3px 6px, rgba(0, 0, 0, 0.06) 0 12px 21px;"
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--on-surface-variant)",
      "background": "var(--wp--preset--color--surface)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--sans)",
      "fontSize": "var(--wp--preset--font-size--body-large)",
      "lineHeight": "var(--wp--custom--typography--leading--normal)",
      "letterSpacing": "0.01em"
    },
    "elements": {
      "h1": {
        "color": {
          "text": "var(--wp--preset--color--on-surface)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontSize": "var(--wp--preset--font-size--display-large)",
          "fontWeight": "400",
          "lineHeight": "var(--wp--custom--typography--leading--tight)"
        }
      },
      "h2": {
        "color": {
          "text": "var(--wp--preset--color--on-surface)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontSize": "var(--wp--preset--font-size--display-medium)",
          "fontWeight": "400",
          "lineHeight": "var(--wp--custom--typography--leading--tight)"
        }
      },
      "h3": {
        "color": {
          "text": "var(--wp--preset--color--on-surface)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontSize": "var(--wp--preset--font-size--display-small)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--typography--leading--tight)"
        }
      },
      "h4": {
        "color": {
          "text": "var(--wp--preset--color--on-surface)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontSize": "var(--wp--preset--font-size--headline-large)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--typography--leading--tight)"
        }
      },
      "h5": {
        "color": {
          "text": "var(--wp--preset--color--on-surface)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontSize": "var(--wp--preset--font-size--headline-medium)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--typography--leading--tight)"
        }
      },
      "h6": {
        "color": {
          "text": "var(--wp--preset--color--on-surface)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontSize": "var(--wp--preset--font-size--headline-small)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--typography--leading--tight)"
        }
      }
    },
    "blocks": {
      "core/button": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontSize": "var(--wp--preset--font-size--body-medium)",
          "fontWeight": "500",
          "lineHeight": "24px",
          "letterSpacing": "0.03em"
        },
        "spacing": {
          "padding": {
            "top": "6px",
            "right": "16px",
            "bottom": "6px",
            "left": "16px"
          }
        }
      },
      "core/navigation": {
        "color": {
          "text": "var(--wp--preset--color--on-surface)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sans)",
          "fontSize": "var(--wp--preset--font-size--body-medium)",
          "fontWeight": "500",
          "lineHeight": "20px",
          "letterSpacing": "0.03em",
          "textTransform": "uppercase"
        }
      }
    }
  }
}
